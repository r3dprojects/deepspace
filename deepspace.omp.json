{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "palette": {
    "path-fg": "#bb9af7",
    "path-bg": "#2c2e3e",
    "os-fg": "#7dcfff",
    "os-bg": "#1a1b26",
    "root-fg": "#96ff9c",
    "root-bg": "#1a1b26",
    "info-fg": "#f7768e",
    "info-bg": "#4a4c70",
    "info-warn": "#c4fbff",
    "git-fg": "#7dcfff",
    "git-bg": "#3b3d57",
    "git-ahead": "#B388FF",
    "git-behind": "#B388FF",
    "git-changed": "#bb9af7",
    "code-green": "#95f5a3",
    "code-red": "#ee3003"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",

          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:os-bg"],
          "template": "{{ if .WSL }}WSL {{ end }}{{ .Icon }} ",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "os",
          "style": "plain",
          "powerline_symbol": "\ue0b0",

          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:path-bg"],
          "template": "{{ if .Root }}<b>{{ .UserName}}@{{ .HostName }}</b>{{ else }}{{ .UserName }}@{{ .HostName }}{{ end }} ",

          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",

          "foreground": "p:path-fg",
          "background": "p:path-bg",
          "foreground_templates": ["p:path-fg"],
          "background_templates ": ["p:path-bg"],
          "template": " \uf07c  {{ .Path }} ",

          "properties": {
            "cache_duration": "none",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",

          "foreground": "p:git-fg",
          "background": "p:git-bg",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-ahead{{ end }}"
          ],

          "template": " \uf418 {{ .Ref }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",

          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": false
          }
        },
        {
          "type": "status",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",

          "foreground": "p:code-green",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:code-red{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }} \uebb4 {{ else }} \uebb4 {{ end }}",

          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        },

        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",

          "foreground": "p:info-fg",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:info-warn{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }}<i>#{{ .Code}}</i> {{ end }}",

          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "leading_diamond": "\ue0b6",
          "template": "\uf017 {{ .FormattedMs }} ",
          "foreground": "p:os-fg",
          "background": "#2c2e3e",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\uE0B0",
          "type": "command",
          "style": "plain",
          "foreground": "#7dcfff",
          "background": "#2c2e3e",
          "properties": {
            "shell": "bash",
            "style": "full",
            "cache_duration": "none",
            "command": "echo $(tput cols </dev/tty 2>/dev/null || echo 80)√ó$(tput lines </dev/tty 2>/dev/null || echo 24)"
          },
          "template": "\ueb4c {{ .Output }} "
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#f36943",
          "background_templates": [
            "{{if lt .Percentage 20}}#ff0000{{end}}",
            "{{if and (ge .Percentage 20) (lt .Percentage 50)}}#ff8800{{end}}",
            "{{if and (ge .Percentage 50) (lt .Percentage 80)}}#ffff00{{end}}",
            "{{if ge .Percentage 80}}#00ff00{{end}}"
          ],
          "properties": {
            "discharging_icon": "üîã",
            "charging_icon": "üîå",
            "charged_icon": "üîã",
            "not_charging_icon": "üö´",
            "display_error": true
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }} "
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ued44 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }} {{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#8ED1F7",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " \udb81\udfd3 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#ffd43b",
          "foreground": "#003768",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " <#4b8bbe>\ue235</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "plain",
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_default": false
          },
          "style": "plain",
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \uf017 {{ .CurrentDate | date \"Jan 02\" }} {{ .CurrentDate | date \"15:04\" }} ",
          "foreground": "#7dcfff",
          "background": "#2c2e3e",
          "type": "time",
          "style": "diamond",
          "min_width": 70
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#7aa2f7",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#7aa2f7>‚ùØ</><#bb9af7>‚ùØ</><#7dcfff>‚ùØ</>",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true
}
