{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "var": {
    "TLRG": "160",
    "TMED": "120",
    "TSML": "80"
  },
  "version": 3,
  "palette": {
    "path-fg": "#bb9af7",
    "path-bg": "#2c2e3e",
    "os-fg": "#7dcfff",
    "os-bg": "#1a1b26",
    "term-fg": "#7dcfff",
    "term-bg": "#2c2e3e",
    "root-fg": "#95f5a3",
    "root-bg": "#1a1b26",
    "info-fg": "#f7768e",
    "info-bg": "#4a4c70",
    "info-warn": "#c4fbff",
    "git-fg": "#7dcfff",
    "git-bg": "#3b3d57",
    "git-ahead": "#00c57a",
    "git-behind": "#007045",
    "git-fg-changed": "#3b3d57",
    "git-changed": "#B388FF",
    "aws-fg": "#000000",
    "aws-bg": "#FFA400",
    "node-fg": "#FFFFFF",
    "node-bg": "#6CA35E",
    "go-fg": "#111111",
    "go-bg": "#8ED1F7",
    "python-fg": "#003768",
    "python-bg": "#ffd43b",
    "terraform-fg": "#5c4ee5",
    "terraform-bg": "#ffffff",
    "code-green": "#95f5a3",
    "code-red": "#ee3003",
    "cycle-0": "#B388FF",
    "cycle-1": "#B388FF",
    "cycle-2": "#aa94ff",
    "cycle-3": "#a1a0ff",
    "cycle-4": "#98acff",
    "cycle-5": "#8fb8ff",
    "cycle-6": "#86c4ff",
    "cycle-7": "#7dcfff",
    "tx": "transparent"
  },

  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",

      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:os-fg",
          "background": "p:tx",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "template": "\u256d\u2500"
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "leading_powerline_symbol": "\u2588",
          "foreground": "p:os-fg",
          "background": "p:term-bg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:path-bg"],
          "template": "{{ if .Root }}<b>{{ .UserName}}@{{ .HostName }}</b>{{ else }}{{ .UserName }}@{{ .HostName }}{{ end }} ",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:git-fg",
          "background": "p:git-bg",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-fg-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-fg-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-fg-changed{{ end }}",
            "{{ if gt .Behind 0 }}p:git-fg-changed{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "template": " \uf418 {{ .Ref }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": false
          }
        }
      ]
    },

    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#f36943",
          "background_templates": [
            "{{if lt .Percentage 20}}#ff0000{{end}}",
            "{{if and (ge .Percentage 20) (lt .Percentage 50)}}#ff8800{{end}}",
            "{{if and (ge .Percentage 50) (lt .Percentage 80)}}#ffff00{{end}}",
            "{{if ge .Percentage 80}}#00ff00{{end}}"
          ],
          "properties": {
            "discharging_icon": "🔋",
            "charging_icon": "🔌",
            "charged_icon": "🔋",
            "not_charging_icon": "🚫",
            "display_error": true
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }} "
        },

        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:node-fg",
          "background": "p:node-bg",
          "template": "\ued44 {{ $cols := .Env.TERMCOLS | trim | default \"80\" | atoi }}{{ if gt $cols (.Var.TLRG | atoi)}} {{ .Full }}{{ else if gt $cols (.Var.TMED | atoi) }} {{ .Major }}.{{ .Minor }}{{ else }}{{ end }}",
          "properties": {
            "fetch_version": true,
            "fetch_package_manager": false
          }
        },

        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:go-fg",
          "background": "p:go-bg",
          "template": "\udb81\udfd3\u2008{{ $cols := .Env.TERMCOLS | trim | default \"80\" | atoi }}{{ if gt $cols (.Var.TLRG | atoi)}}{{ .Full }}{{ else if gt $cols (.Var.TMED | atoi) }}{{ .Major }}.{{ .Minor }}{{ else }}{{ end }}",
          "properties": {
            "fetch_version": true,
            "cache_duration": "none"
          }
        },

        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:python-fg",
          "background": "p:python-bg",
          "template": "\ued1b {{ $cols := .Env.TERMCOLS | trim | default \"80\" | atoi }}{{ if gt $cols (.Var.TLRG | atoi)}}{{ .Full }}{{ else if gt $cols (.Var.TMED | atoi) }}{{ .Major }}.{{ .Minor }}{{ else }}{{ .Major }}{{ end }}",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": false,
            "fetch_version": true
          }
        },
        {
          "type": "terraform",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:terraform-fg",
          "background": "p:terraform-bg",
          "template": "\ue8bd{{ $cols := .Env.TERMCOLS | trim | default \"80\" | atoi }}{{ if gt $cols (.Var.TLRG | atoi)}}{{ .Version }} {{ .WorkspaceName }}{{ else if gt $cols (.Var.TMED | atoi) }}{{ .Version }} {{ .WorkspaceName }}{{ else }}{{ end }}",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "aws",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:aws-fg",
          "background": "p:aws-bg",
          "background_templates": [
            "{{if contains \"default\" .Profile}}p:aws-bg{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "template": "\uf0ef ",
          "properties": {
            "display_default": true
          }
        },
        {
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\uE0B0",
          "type": "command",
          "style": "plain",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "properties": {
            "shell": "bash",
            "style": "full",
            "cache_duration": "none",
            "command": "echo $(tput cols </dev/tty 2>/dev/null || echo 80)×$(tput lines </dev/tty 2>/dev/null || echo 24)"
          },
          "template": " \ueb4c {{ .Output }} "
        },
        {
          "trailing_diamond": "\ue0b4",
          "template": "{{ .CurrentDate | date \"15:04PM\" }}",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "type": "time",
          "style": "diamond",
          "min_width": 126,
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:term-fg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],

          "template": "\u2500\u256e"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:os-fg",
          "background": "p:tx",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:term-bg"],
          "template": "{{ if .WSL }}WSL {{ end }}{{ .Icon }} ",
          "properties": {
            "cache_duration": "none"
          }
        },

        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:path-fg",
          "background": "p:path-bg",
          "foreground_templates": ["p:path-fg"],
          "background_templates ": ["p:path-bg"],
          "template": " \uf07c  {{ .Path }} ",
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07c ",
            "cycle": [
              "p:cycle-0",
              "p:cycle-1",
              "p:cycle-1",
              "p:cycle-2",
              "p:cycle-2",
              "p:cycle-3",
              "p:cycle-4",
              "p:cycle-5",
              "p:cycle-6",
              "p:cycle-7"
            ],
            "cycle_folder_separator": true,
            "style": "full"
          }
        },

        {
          "type": "status",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "foreground": "p:code-green",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:code-red{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }} \uebb4 {{ else }} \uebb4 {{ end }}",
          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:info-fg",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:info-warn{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }}<i>#{{ .Code}}</i> {{ end }}",
          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": "\uf017 {{ .FormattedMs }} ",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "type": "executiontime",
          "style": "diamond",
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          }
        },

        {
          "type": "sysinfo",
          "powerline_symbol": "",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "template": "\ue266 {{ round .PhysicalPercentUsed .Precision  }}% \udb80\uddbc {{ round .SwapPercentUsed .Precision }}% ",
          "properties": {
            "precision": 0,
            "cache_duration": "none"
          },
          "style": "plain"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:term-fg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "template": "\u2500\u256f"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",

          "foreground": "#7aa2f7",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "template": "\u2570\u2500",
          "properties": {
            "cache_duration": "none"
          }
        },

        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#7aa2f7>❯</><#bb9af7>❯</><#7dcfff>❯</>",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true
}
