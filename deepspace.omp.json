{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "palette": {
    "path-fg": "#bb9af7",
    "path-bg": "#2c2e3e",
    "os-fg": "#7dcfff",
    "os-bg": "#1a1b26",
    "term-fg": "#7dcfff",
    "term-bg": "#2c2e3e",
    "root-fg": "#96ff9c",
    "root-bg": "#1a1b26",
    "info-fg": "#f7768e",
    "info-bg": "#4a4c70",
    "info-warn": "#c4fbff",
    "git-fg": "#7dcfff",
    "git-bg": "#3b3d57",
    "git-ahead": "#00c57a",
    "git-behind": "#007045",
    "git-fg-changed": "#3b3d57",
    "git-changed": "#B388FF",
    "aws-fg": "#000000",
    "aws-bg": "#FFA400",
    "node-fg": "#FFFFFF",
    "node-bg": "#6CA35E",
    "go-fg": "#111111",
    "go-bg": "#8ED1F7",
    "python-fg": "#003768",
    "python-bg": "#ffd43b",
    "code-green": "#95f5a3",
    "code-red": "#ee3003"
  },
  "var": {
    "min-sml": 50,
    "max-sml": 80,
    "min-med": 81,
    "max-med": 120,
    "min-lrg": 121,
    "max-lrg": 999,
    "size": "medium",
    "widths": {
      "small": { "max": 80, "min": 50 },
      "medium": { "max": 120, "min": 81 },
      "large": { "max": 999, "min": 121 }
    }
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",

      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:os-fg",
          "template": "\u256d\u2500"
        },
        {
          "type": "os",
          "style": "powerline",
          "trailing_powerline_symbol": "\ue0b0",
          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:path-bg"],
          "template": "{{ if .Root }}<b>{{ .UserName}}@{{ .HostName }}</b>{{ else }}{{ .UserName }}@{{ .HostName }}{{ end }} ",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:git-fg",
          "background": "p:git-bg",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-fg-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-fg-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-fg-changed{{ end }}",
            "{{ if gt .Behind 0 }}p:git-fg-changed{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-changed{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "template": " \uf418 {{ .Ref }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": false
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#f36943",
          "background_templates": [
            "{{if lt .Percentage 20}}#ff0000{{end}}",
            "{{if and (ge .Percentage 20) (lt .Percentage 50)}}#ff8800{{end}}",
            "{{if and (ge .Percentage 50) (lt .Percentage 80)}}#ffff00{{end}}",
            "{{if ge .Percentage 80}}#00ff00{{end}}"
          ],
          "properties": {
            "discharging_icon": "üîã",
            "charging_icon": "üîå",
            "charged_icon": "üîã",
            "not_charging_icon": "üö´",
            "display_error": true
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }} "
        },

        {
          "type": "node",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:node-fg",
          "background": "p:node-bg",
          "template": " \ued44\u2008  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }} {{ .Full }} ",
          "properties": {
            "fetch_version": true
          },

          "min_width": 100
        },
        {
          "type": "node",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:node-fg",
          "background": "p:node-bg",
          "template": " \ued44\u2008 ",
          "properties": {
            "fetch_version": false
          },
          "max_width": 99
        },

        {
          "type": "go",
          "style": "plain",
          "foreground": "p:go-fg",
          "background": "p:go-bg",
          "template": "\u2008\udb81\udfd3\u2008\u2008 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": false
          },
          "min_width": 100
        },

        {
          "type": "go",
          "style": "plain",
          "foreground": "p:go-fg",
          "background": "p:go-bg",
          "template": "\u2008\udb81\udfd3\u2008\u2008",
          "properties": {
            "fetch_version": false
          },
          "max_width": 99
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:python-fg",
          "background": "p:python-bg",
          "template": " \ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}3.25.1 ",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": false,
            "fetch_version": false
          },
          "min_width": 100
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:python-fg",
          "background": "p:python-bg",
          "template": " \ue73c ",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": false,
            "fetch_version": false
          },
          "max_width": 99
        },
        {
          "type": "aws",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:aws-fg",
          "background": "p:aws-bg",
          "background_templates": [
            "{{if contains \"default\" .Profile}}p:aws-bg{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "template": " \udb83\ude0f ",
          "properties": {
            "display_default": true
          }
        },
        {
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\uE0B0",
          "type": "command",
          "style": "plain",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "properties": {
            "shell": "bash",
            "style": "full",
            "cache_duration": "none",
            "command": "echo $(tput cols </dev/tty 2>/dev/null || echo 80)√ó$(tput lines </dev/tty 2>/dev/null || echo 24)"
          },
          "template": " \ueb4c {{ .Output }} "
        },
        {
          "trailing_diamond": "\ue0b4",
          "template": "{{ .CurrentDate | date \"15:04PM\" }}",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "type": "time",
          "style": "diamond",
          "min_width": 126,
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:term-fg",

          "template": "\u2500\u256e"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ else }}p:os-fg{{ end }}"
          ],
          "background_templates ": ["p:os-bg"],
          "template": "{{ if .WSL }}WSL {{ end }}{{ .Icon }} ",
          "properties": {
            "cache_duration": "none"
          }
        },

        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:path-fg",
          "background": "p:path-bg",
          "foreground_templates": ["p:path-fg"],
          "background_templates ": ["p:path-bg"],
          "template": " \uf07c  {{ .Path }} ",
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "min_width": 120
        },

        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:path-fg",
          "background": "p:path-bg",
          "foreground_templates": ["p:path-fg"],
          "background_templates ": ["p:path-bg"],
          "template": " \uf07c  {{ .Path }} ",
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "max_width": 119
        },

        {
          "type": "status",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "foreground": "p:code-green",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:code-red{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }} \uebb4 {{ else }} \uebb4 {{ end }}",
          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:info-fg",
          "background": "p:info-bg",
          "foreground_templates": ["{{ if gt .Code 0 }}p:info-warn{{ end }}"],
          "background_templates": ["{{ if gt .Code 0 }}p:info-bg{{ end }}"],
          "template": "{{ if gt .Code 0 }}<i>#{{ .Code}}</i> {{ end }}",
          "properties.display_exit": false,
          "properties": {
            "prefix": " ",
            "postfix": " \u2800",
            "display_exit": false,
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": "\uf017 {{ .FormattedMs }} ",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "type": "executiontime",
          "style": "diamond",
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          }
        },

        {
          "type": "sysinfo",
          "powerline_symbol": "",
          "foreground": "p:term-fg",
          "background": "p:term-bg",
          "template": "\ue266 {{ round .PhysicalPercentUsed .Precision  }}% \udb80\uddbc {{ round .SwapPercentUsed .Precision }}% ",
          "properties": {
            "precision": 0,
            "cache_duration": "none"
          },
          "style": "plain"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:term-fg",
          "template": "\u2500\u256f"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#7aa2f7",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#7aa2f7>‚ùØ</><#bb9af7>‚ùØ</><#7dcfff>‚ùØ</>",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true
}
