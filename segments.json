[
  {
    "type": "azfunc",
    "style": "plain",
    "powerline_symbol": "\ue0b0",
    "foreground": "#ffffff",
    "background": "#FEAC19",
    "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",

    "properties": {
      "display_mode": "files",
      "fetch_version": false
    }
  },
  {
    "type": "ruby",
    "style": "plain",
    "powerline_symbol": "\ue0b0",

    "background": "#AE1401",
    "foreground": "#ffffff",
    "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",

    "properties": {
      "display_mode": "files",
      "fetch_version": true
    }
  },
  {
    "type": "python",
    "style": "plain",
    "powerline_symbol": "\ue0b0",

    "foreground": "p:python-fg",
    "background": "p:python-bg",
    "template": " <#4b8bbe>\ue235</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
    "properties": {
      "display_mode": "files",
      "fetch_virtual_env": false
    }
  },
  {
    "type": "node",
    "style": "plain",
    "powerline_symbol": "\ue0b0",
    "foreground": "#ffffff",
    "background": "#6CA35E",
    "template": " \ued44 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }} {{ .Full }} ",

    "properties": {
      "fetch_version": true
    }
  },
  {
    "type": "go",
    "style": "plain",
    "powerline_symbol": "\ue0b0",

    "foreground": "p:go-fg",
    "background": "p:go-bg",
    "template": " \udb81\udfd3 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",

    "properties": {
      "fetch_version": true
    }
  },
  {
    "type": "text",
    "style": "plain",
    "foreground": "#ffffff",
    "background": "#000000",
    "template": "DEBUG: {{ .Env.COLUMNS }} "
  },
  {
    "type": "command",
    "style": "plain",
    "foreground": "#ffffff",
    "properties": {
      "command": "echo $(tput cols </dev/tty 2>/dev/null || echo 80)"
    },
    "template": "Width: {{ .Output }}"
  },

  {
    "template": " \ued44\u2008 {{ .Var.TLRG }} {{ .Var.TMED }} {{ .Var.TSML }}"
  }
]
